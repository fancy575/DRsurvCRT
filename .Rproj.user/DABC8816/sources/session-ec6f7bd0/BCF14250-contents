remotes::install_github("fancy575/DRsurvCRT")
library(DRsurvCRT)
data(dat)
fit_spce <- DRsurvfit(
  data = dat,
  formula = Surv(time, event) ~ W1 + W2 + Z1 + Z2 + cluster(M),
  intv = "A",
  method = "marginal",
  estimand = "SPCE",
  variance = "jackknife"
)
summary(fit_spce, digits = 3, times = c(0.5, 1, 1.5, 2))
